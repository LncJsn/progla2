#include<stdio.h>
#include<iostream>
#include<string>
using namespace std;

class person{
	public:
		string name;
		int age;
};

class student : public person{
	public:
		int stud_num;
		int set_stud_num(int s_num){
			stud_num = s_num;
		};
		int get_stud_num(){
			return stud_num;
		};
};

int main(){
	student stud1[3];
	int i;
	for(int i=0; i<3; i++){
		cout << "Input num "<<i+1;
		cin >> stud1[i].stud_num;
	}
	cout << "Me\n";
	for(int i=0; i<3; i++){
		cout << stud1[i].stud_num;
	}
}
